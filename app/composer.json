{
  "name": "docker-template/app",
  "type": "project",
  "require": {
    "nikic/fast-route": "^1.3",
    "phpmailer/phpmailer": "^6.8",
    "vlucas/phpdotenv": "^5.5"
  },
  "autoload": {
    "psr-4": {
